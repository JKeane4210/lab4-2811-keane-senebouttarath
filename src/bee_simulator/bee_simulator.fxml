<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<GridPane alignment="center" hgap="10" onKeyPressed="#onKeyPressed" vgap="10" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bee_simulator.Garden">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <HBox>
         <Pane id="theGarden" fx:id="theGarden" prefHeight="700.0" prefWidth="700.0">

         </Pane>
         <VBox id="natureBookPane" fx:id="natureBookVBox" prefHeight="700.0" prefWidth="200.0" alignment="TOP_CENTER" spacing="25">
            <style>
               -fx-background-color: white
            </style>
            <children>
               <Label layoutX="50.0" layoutY="550.0" scaleZ="2.0" text="Nature Book" textFill="black" translateZ="1.0" alignment="TOP_CENTER">
                  <font>
                     <Font name="Poor Richard" size="18.0" />
                  </font>
               </Label>
            </children>
         </VBox>
      </HBox>
   </children>
</GridPane>
